{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/live-stream-backend/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/live-stream-backend/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/live-stream-backend",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "live-stream-backend": {
      "type": "application",
      "root": "apps/live-stream-backend",
      "entryFile": "main",
      "sourceRoot": "apps/live-stream-backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/live-stream-backend/tsconfig.app.json"
      }
    },
    "video-processing-worker": {
      "type": "application",
      "root": "apps/video-processing-worker",
      "entryFile": "main",
      "sourceRoot": "apps/video-processing-worker/src",
      "compilerOptions": {
        "tsConfigPath": "apps/video-processing-worker/tsconfig.app.json"
      }
    },
    "video-upload-service": {
      "type": "application",
      "root": "apps/video-upload-service",
      "entryFile": "main",
      "sourceRoot": "apps/video-upload-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/video-upload-service/tsconfig.app.json"
      }
    }
  }
}